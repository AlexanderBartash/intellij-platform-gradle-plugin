<idea-plugin>
    <id>org.intellij.examples.dependencies.plugin</id>
    <name>Kotlin plugin</name>
    <version>1.0</version>
    <vendor url="https://jetbrains.com">JetBrains</vendor>
    <description>A plugin that depends on built-in and external plugins</description>
    <change-notes/>

    <actions>
        <action class="org.intellij.examples.kotlin.plugin.KotlinHelloAction" id="SayHelloToKotlin"
                text="Say Hello To Kotlin">
            <add-to-group group-id="ToolsMenu"/>
            <add-to-group group-id="WelcomeScreen.QuickStart"/>
        </action>
    </actions>
</idea-plugin>